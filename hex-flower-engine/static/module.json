    "version": "#{VERSION}#",
    "compatibility": {
        "minimum": "10",
        "verified": "11"
    },
    "authors": [
        {
            "name": "Felipe Alves",
            "email": "",
            "url": ""
        }
    ],
    "esmodules": [
        "dist/hex-flower-engine.js"
    ],
    "styles": [
        "dist/styles/module.css"
    ],
    "packs": [
